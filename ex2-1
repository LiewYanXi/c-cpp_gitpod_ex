#include <stdio.h>

int main()
{
    int     i = 10;
    int     x = 10;

    printf(" please input a integer_1 \n");
    scanf("%d",&i);
    printf(" your input integer_1 = %d\n",i);
    
    printf(" please input a integer_2 \n");
    scanf("%d",&x);
    printf(" your input integer_2 = %d\n",x);
    
    printf(" your input integer-3 = %d\n",(i * i) + (x * x));
    

   
    return 0;
}
